config_version=5

[application]

config/name="rogue_slg"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.5", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Current="*res://scripts/global/current.gd"
SceneManager="*res://scripts/global/scene_manager.gd"
Tools="*res://scripts/global/tools.gd"
EventBus="*res://scripts/global/event_bus.gd"
CursorManager="*res://scripts/global/cursor_manager.gd"
BuffSystem="*res://scripts/buff_system.gd"
EffectManager="*res://scripts/global/effect_manager.gd"
VFX="*res://addons/vfx_library/vfx.gd"
EnvVFX="*res://addons/vfx_library/env_vfx.gd"

[display]

window/stretch/mode="viewport"
mouse_cursor/custom_image="res://images/mouse_icon/pointer_scifi_a.svg"

[editor_plugins]

enabled=PackedStringArray("res://addons/csv-data-importer/plugin.cfg", "res://addons/godot_sfxr/plugin.cfg", "res://addons/script-ide/plugin.cfg", "res://addons/vfx_library/plugin.cfg")

